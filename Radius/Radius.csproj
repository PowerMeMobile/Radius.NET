<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Company>PowerMeMobile</Company>
    <Description>It's adaptation of Radius package (forked from https://github.com/frontporch/Radius.NET) for .Net Standard.</Description>
    <Authors>PowerMeMobile</Authors>
    <Copyright>PowerMeMobile 2020</Copyright>
    <PackageProjectUrl>https://github.com/PowerMeMobile/Radius.NET</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PowerMeMobile/Radius.NET</RepositoryUrl>
    <PackageTags>PMM,Radius</PackageTags>
    <PackageReleaseNotes>Set target framework to .net standard 2.0</PackageReleaseNotes>
  </PropertyGroup>

</Project>
